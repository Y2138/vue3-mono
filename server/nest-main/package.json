{
  "name": "nest-main",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:debug": "vitest --inspect-brk",
    "test:e2e": "vitest run --config ./test/vitest-e2e.config.ts",
    "test:ui": "vitest --ui",
    "proto:gen": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/shared --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true --proto_path=../../protos ../../protos/*.proto",
    "proto:watch": "nodemon --watch ../../protos --ext proto --exec \"npm run proto:gen\"",
    "dev:with-proto": "concurrently \"npm run proto:watch\" \"npm run start:dev\"",
    "test:grpc": "ts-node src/utils/grpc-client/connection-test.ts",
    "test:grpc:simple": "ts-node src/utils/grpc-client/connection-test.ts simple",
    "test:grpc:batch": "ts-node src/utils/grpc-client/connection-test.ts batch",
    "test:performance": "ts-node src/scripts/performance-test.ts",
    "optimize:errors": "ts-node src/scripts/error-handling-optimizer.ts",
    "validate:stage5": "ts-node src/scripts/validate-stage5.ts"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.2",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.12",
    "@wecom/crypto": "^1.0.1",
    "alipay-sdk": "^4.13.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "ioredis": "^5.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^18.0.0",
    "ts-proto": "^2.7.3",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",

    "@types/node": "^22.14.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.8.2"
  }
}
